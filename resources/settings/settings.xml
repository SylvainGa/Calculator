<settings>
	<setting propertyKey="@Properties.AppVersion" title="@Strings.AppNameVersion">
		<settingConfig type="alphaNumeric" readonly="true"/>
	</setting>

    <setting propertyKey="@Properties.historySize" title="@Strings.setting_historySize">
		<settingConfig type="numeric" min="0" max="50" required="true"/>
    </setting>

    <setting propertyKey="@Properties.panelOrder" title="@Strings.setting_panelOrder">
		<settingConfig type="alphaNumeric" required="true"/>
    </setting>

    <setting propertyKey="@Properties.vibrateOnTouch" title="@Strings.setting_vibrateOnTouch">
		<settingConfig type="boolean" required="true"/>
    </setting>
</settings>
